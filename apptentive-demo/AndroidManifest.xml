<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.apptentive.android.demo"
          android:versionCode="1"
          android:versionName="1.0">
	<uses-permission android:name="android.permission.INTERNET"/>
	<uses-permission android:name="android.permission.GET_ACCOUNTS"/>
	<application android:label="@string/demo_app_name" android:icon="@drawable/icon">
		<activity android:name=".DemoActivity"
		          android:label="@string/demo_activity_name"
		          android:configChanges="orientation|keyboardHidden">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>
				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
		<activity android:name="com.apptentive.android.sdk.ApptentiveActivity"/>
	</application>
</manifest>